{
  "name": "coinglue",
  "version": "1.0.0",
  "description": "a multi-crypto currency framework that glues other wallets together for a centralized api",
  "main": "index.js",
  "bin": {
    "coinglue": "./bin/coinglue",
    "coincli": "./bin/coincli",
    "coinhook": "./bin/coinhook"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cover": "mocha test/**/*.spec.js",
    "dev:start": "NODE_ENV=development ./bin/coinglue start --debug",
    "dev:stop": "./bin/coinglue stop",
    "dev:reset": "rm -rf .logs ./src/config/dev/seed.js && npm run dev:resetdb",
    "dev:resetdb": "rm -rf ./src/services/dev/btc ./src/services/dev/doge ./src/services/dev/eth ./src/services/dev/ltc ./src/services/dev/tbtc ./src/services/dev/bch",
    "dev:reseteth": "rm -rf ./src/services/dev/ethblocks"
  },
  "keywords": [
    "cryptocurrency",
    "wallet",
    "bitcoin"
  ],
  "author": "unibtc@gmail.com",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "bip44-constants": "^4.0.1",
    "bitcoin": "^3.0.1",
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "coinblocks": "^1.0.1",
    "coinjs-lib": "^1.0.1",
    "coinnames": "^1.0.1",
    "coinup": "^1.0.0",
    "commander": "^2.13.0",
    "console-stamp": "^0.2.6",
    "ethereumjs-tx": "^1.3.3",
    "eventemitter3": "^3.0.1",
    "fs-extra": "^5.0.0",
    "koa": "^2.4.1",
    "koa-jsonrpc": "^1.1.1",
    "needle": "^2.2.0",
    "node-dir": "^0.1.17",
    "node-lmdb": "^0.4.13",
    "readline-sync": "^1.4.7",
    "web3": "^0.20.4"
  }
}
